%ul.list-unstyled.col-sm-12
  %li
    = form.hidden_field(attribute, required: false, class: "form-control", value: true)
  %li
    = form.select(options_attribute, options_for_select(search_form.search_predicates_for(attribute), search_form.send(options_attribute)), {include_blank: true}, {class: "form-control"})
